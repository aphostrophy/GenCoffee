{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".ios.js", ".android.js", ".js", ".jsx", ".ts", ".tsx"],
        "alias": {
          "@api": "./src/api",
          "@action-creators": "./src/action-creators",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@hooks": "./src/hooks",
          "@navigations": "./src/navigations",
          "@slices": "./src/slices",
          "@selectors": "./src/selectors",
          "@screens": "./src/screens",
          "@services": "./src/services",
          "@stores": "./src/stores",
          "@styles": "./src/styles",
          "@types": "./src/types",
          "@utils": "./src/utils"
        }
      }
    ]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript",
    "@babel/preset-flow"
  ]
}
